{# set classes = (node.type.entity.label != 'Organization' or 'Regional Office' or 'Office Page') ? 'grid-container' : '' #}
{% set classes = '' %}

{% set content_id = 'ia-main-content' %}

<article{{ attributes.addClass(classes) }}>
    <div {{ attributes.setAttribute('id', content_id) }} class="node-inner node-content">
        {{ title_prefix }}
        {% if not page %}
        <h2{{ title_attributes }}>
            <a href="{{ node_url }}">
                {{ label }}
            </a>
        </h2>
        {% endif %}
        {{ title_suffix }}
        {% if node.isPublished() == false %}
            <div class="unpublished">Unpublished</div>
        {% endif %}
        {% block body %}{% endblock %}
    </div>
</article>
{% block schema %}{% endblock %}