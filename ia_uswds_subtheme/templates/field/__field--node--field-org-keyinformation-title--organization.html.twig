{#
/**
 * @file
 * Theme override for:
 * Node Type = Organization
 * Field = 	Key Information	 (field_org_keyinformation); Key Information Title (field_org_keyinformation_title)
 */
#}
{%
    set title_classes = [
    label_display == 'visually_hidden' ? 'usa-sr-only'
]
%}

{% if label_hidden==false %}
    <h2 class="call-out" {{ title_attributes.addClass(title_classes)}}>{{ label }}</h2>
{% endif %}


<section id="key-information" class="margin-y-4">
    <div class="grid-row grid-gap-4">
        <div class="grid-col-12">
            <div class="usa-summary-box" role="complementary">
                <div class="usa-summary-box__body">
                    {% for item in items %}
                        <div{{ item.attributes }}>{{ item.content }}</div>
                    {% endfor %}
                </div>
            </div>
        </div> <!-- /.grid-col-12 -->
    </div><!-- /.grid-row grid-gap-4 -->
</section>