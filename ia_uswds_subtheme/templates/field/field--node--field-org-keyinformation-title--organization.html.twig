{#
/**
 * @file
 * Theme override for:
 * Node Type = Organization
 * Field = 	Key Information	 (field_org_keyinformation); Key Information Title (field_org_keyinformation_title)
 */
#}
{%
    set title_classes = [
    label_display == 'visually_hidden' ? 'usa-sr-only'
]
%}

{% if label_hidden==false %}
    <h2 class="call-out" {{ title_attributes.addClass(title_classes)}}>{{ label }}</h2>
{% endif %}
<strong>
    {% for item in items %}
        <div{{ item.attributes }}>{{ item.content }}</div>
    {% endfor %}
</strong>