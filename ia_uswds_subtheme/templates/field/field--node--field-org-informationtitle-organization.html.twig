{#
/**
 * @file
 * Theme override for:
 * Node Type = Organization
 * Field = Supplementary Title (field_org_informationtitle)
 */
#}
{%
    set title_classes = [
    label_display == 'visually_hidden' ? 'usa-sr-only'
]
%}

{% if label_hidden==false %}
    <h2 class="call-out" {{ title_attributes.addClass(title_classes)}}>{{ label }}</h2>
{% endif %}

{% for item in items %}
    <div{{ item.attributes.addClass(item_classes) }}>{{ item.content }}</div>
{% endfor %}